{
  "name": "cpplanta-backend",
  "author": "AGES",
  "description": "Backend for CPPlanta project",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "typescript",
    "prisma",
    "swagger",
    "passport",
    "nodejs",
    "postgresql"
  ],
  "private": true,
  "repository": {
    "type": "gitlab",
    "url": "https://tools.ages.pucrs.br/cp-planta/backend"
  },
  "bugs": {
    "url": "https://tools.ages.pucrs.br/cp-planta/backend/-/issues"
  },
  "homepage": "https://www.ages.pucrs.br/projetos-2/cpplanta/",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "NODE_ENV=production nest start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --passWithNoTests",
    "build": "npm run lint && nest build",
    "build:only": "nest build",
    "lint": "eslint src --max-warnings=0",
    "prettier": "npx prettier --write --ignore-unknown .",
    "format": "npm run prettier",
    "commit": "git-cz",
    "prepare": "if [ \"$NODE_ENV\" = \"development\" ]; then husky install; fi",
    "release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "pretty-quick --staged",
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "detect-port": "^1.6.1",
    "dotenv": "^16.4.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/format": "^19.3.0",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-nestjs": "^0.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "3.3.3",
    "pretty-quick": "^4.0.0",
    "prisma-erd-generator": "^1.11.2",
    "semantic-release": "^24.1.0",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.4",
    "typescript": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
